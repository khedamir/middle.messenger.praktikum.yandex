<div class='chats'>
  <div class='chats-header'>
    {{{Button
      classes='chats-header__button'
      label='Profile'
      onClick=toProfile
    }}}
  </div>
  {{{Search}}}
  <div class='chats-items'>
    {{#each chats}}
      {{{ChatItem name=this.name message=this.message time=this.time}}}
    {{/each}}
  </div>
</div>